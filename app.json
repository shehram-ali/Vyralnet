{
  "expo": {
    "name": "Vyralnet",
    "slug": "vyralnet",
    "version": "1.0.0",
    "scheme": "vyralnet",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#FFFFFF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vyralnet.app"
    },
    "android": {
      "package": "com.vyralnet.app"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d00515c8-8cc3-4e56-b385-408a38fd6475"
      }
    }
  }
}
